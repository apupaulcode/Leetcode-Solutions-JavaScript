Solution-1

var findKthLargest = function (nums, k){
    for(let i=Math.floor(nums.length/2)-1;i>=0;i--){
        heapify(nums,nums.length,i)
    }
    for(let i=nums.length-1;i>=nums.length-k-1 && i>=0;i--){
        swap(nums,0,i)
        heapify(nums,i,0)
    }
    return nums[nums.length-k]
}
function heapify(nums,length,i){
    let left=2*i+1,right=2*i+2
    if(left>=length) return
    if(nums[i]<nums[left] && (right>=length||nums[left]>nums[right])){
        swap(nums,left,i)
        heapify(nums,length,left)
    }else if(right<length && nums[right]>nums[i]){
        swap(nums,right,i)
        heapify(nums,length,right)
    }
}

function swap(nums,a,b){
    const temp=nums[a]
    nums[a]=nums[b]
    nums[b]=temp
}

console.log(findKthLargest([3,2,3,1,2,4,5,5,6],4))


Solution-2

function findKthLargest(arr,k){
    let pq=[]
    arr.forEach((val)=>{
        pq.push(val)
        pq.sort((a,b)=>a-b)
        if(pq.length>k){
            pq.shift()
        }
    })
    return pq[0]
}

