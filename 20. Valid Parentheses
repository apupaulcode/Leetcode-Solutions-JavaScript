Solution-1
var isValid = function (s) {
  var stack = [];
  for (var i = 0; i < s.length; i++) {
    var elem = s.charAt(i);
    if (elem === ")" || elem === "]" || elem === "}") {
      if (stack.length === 0) return false;
      var lasPar = stack.shift();
      if (!valid(lasPar, elem)) return false;
    } else {
      stack.unshift(elem);
    }
  }

  return stack.length === 0;
};

var valid = function (parOpen, parClose) {
  return (
    (parOpen === "(" && parClose === ")") ||
    (parOpen === "[" && parClose === "]") ||
    (parOpen === "{" && parClose === "}")
  );
};

console.log(isValid("()[]{}"))
console.log(isValid("()[]{"))

Solution-2
function isValid(s) {
  const stack = [];
  const closeToOpen = {
    ")": "(",
    "]": "[",
    "}": "{",
  };
  for(let c of s){
    if(closeToOpen[c]){
        if(stack.length>0 && stack[stack.length-1]===closeToOpen[c]){
            stack.pop()
        }else{
            return false
        }
    }else{
        stack.push(c)
    }
  }
  return stack.length===0
}
